{
    "name": "@awes-io/form-builder",
    "version": "0.0.1",
    "main": "dist/form-builder.ssr.js",
    "module": "dist/form-builder.esm.js",
    "unpkg": "dist/form-builder.min.js",
    "files": [
        "dist/*",
        "src/**/*.vue"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "yarn clean && cross-env NODE_ENV=production rollup -c ./build/rollup.config.production.js",
        "dev": "rollup -c ./build/rollup.config.js -w",
        "reload": "browser-sync start -c browser-sync.config.js",
        "watch": "yarn clean && concurrently \"yarn dev\" \"yarn reload\"",
        "lint": "eslint --ext .js,.vue ./src ./tests",
        "lint:fix": "eslint --ext .js,.vue ./src ./tests --fix",
        "test": "jest",
        "test:watch": "jest -w",
        "version": "auto-changelog -p && git add CHANGELOG.md"
    },
    "repository": {
        "type": "git",
        "url": "git@git.awescode.com:awes-io/form-builder.git"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "yarn lint && yarn test"
        }
    },
    "license": "MIT",
    "dependencies": {
        "core-js": "^2.6.5",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "auto-changelog": "^1.14.1",
        "autoprefixer": "^9.6.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "browser-sync": "^2.26.7",
        "concurrently": "^4.1.1",
        "cross-env": "^5.2.0",
        "cssnano": "^4.1.10",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.2.3",
        "husky": "^3.0.1",
        "jest": "^24.8.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "postcss-import": "^12.0.1",
        "prettier": "^1.18.2",
        "rimraf": "^2.6.3",
        "rollup": "^1.17.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.0.1",
        "rollup-plugin-css-only": "^1.0.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-license": "^0.9.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-terser": "^5.1.1",
        "rollup-plugin-vue": "^5.0.1",
        "vue-jest": "^3.0.4",
        "vue-template-compiler": "^2.6.10"
    }
}
